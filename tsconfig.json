{
  "include": [
    "."
  ],
  "compilerOptions": {
    "rootDir": ".",
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "katalon/*": [
        "lib/katalon/*"
      ]
    }
  }
}